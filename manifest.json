{
  "manifest_version": 2,
  "name": "plug-dj-rekey",
  "description": "An addon to use an alternate google API key for youtube requests on plug.dj",
  "version": "0.1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "*://plug.dj/*",
    "*://content.googleapis.com/youtube/*",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "applications": {
    "gecko": {
      "id": "plug-dj-rekey-addon@euank.com"
    }
  }
}
